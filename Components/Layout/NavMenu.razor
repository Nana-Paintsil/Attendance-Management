@code {
    private bool collapseNavMenu = true;
    private bool showLogoutForm = false;
    private string NavMenuCssClass => collapseNavMenu ? "collapse" : null;

    private void ToggleNavMenu()
    {
        collapseNavMenu = !collapseNavMenu;
    }

    private async void HandleIsVisibleChanged(bool isVisible)
    {
        showLogoutForm = isVisible;
        if (showLogoutForm)
        {
            await JSRuntime.InvokeVoidAsync("removePositionStyles");
        }else
        {
           await JSRuntime.InvokeVoidAsync("setPositionStyles");
        }
    }

    private async void ToggleInvalidCredentials()
    {
        showLogoutForm = !showLogoutForm;
        if (showLogoutForm)
        {
            await JSRuntime.InvokeVoidAsync("removePositionStyles");
        }
        else
        {
           await JSRuntime.InvokeVoidAsync("setPositionStyles");
        }
    }

    private async Task Logout()
    {   
        NavigationManager.NavigateTo("/");
        showLogoutForm = false;
    }
}
